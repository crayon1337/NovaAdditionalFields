{
    "name": "crayon/nova-additional-fields",
    "description": "A Laravel Nova package that allows you to dynamically append inputs to your resources.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Crayon\\AdditionalFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crayon\\AdditionalFields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
